require "spec_helper"

describe Array, "#random" do

  property("empty") { fails? { [].random }}
    .check

  property("member?"){|xs| bind(guard(!xs.empty?)) { bind(xs.random){|x| unit(xs.member?(x)) }}}
    .check { Array.random { Integer.random }}
    .check { Array.random { String.random }}

end
